
package day081;


public class Day081 {
 
    public static void main(String[] args) {
        int angka = 5;
        for (int i = 1; i <= angka; i++) {
            for (int j = 1; j < i; j++) {
                System.out.print("o ");
            }
        System.out.println("o");
        }
    }
    
}
