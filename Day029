#include <iostream>

using namespace std;
int main() 
{
    int x = 50;
    int y = 5;
    int z = x + y;
  cout<<"*************************"<<endl;
  cout<<"***Program Penjumlahan***"<<endl;
  cout<<"*************************"<<endl;
  cout<<"x = "<<x<<endl;
  cout<<"y = "<<y<<endl;
  cout<<x<<" + "<<y<<" = "<<z<<endl;

  return 0;
}
