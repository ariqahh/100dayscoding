
package day093;

public class Day093 {

    
    public static void main(String[] args) {
        //luas = phi x r x r
        //keliling = phi x diameter
        // diameter = 2 x r
        final double phi = 3.14; //konstanta atau nilai mutlak
        double luas, keliling, r, diameter;
        r = 5;
        diameter = 2*r;
        luas = phi*r*r;
        keliling = phi*diameter;
        System.out.println("Mencari luas dan keliling lingkaran");
        System.out.println("luas = " + luas);
        System.out.println("Keliling = " + keliling);
    }
    
}
