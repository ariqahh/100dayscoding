#include <iostream>
using namespace std;
int main()
{
  
  int i;
  double d;
  char c;
  char s;

  
  i = 88;
  d = 4.675;
  c = 'c';
  s = 'p';

  // menampilkan nilai variabel
  cout<<"Nilai i: "<<i<<endl;
  cout<<"Nilai d: "<<d<<endl;
  cout<<"Nilai c: "<<c<<endl;
  cout<<"Nilai s: "<<s<<endl;

  return 0;	
}
