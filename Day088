
package day088;

public class Day088 {

    public static void main(String[] args) {
        int luas, keliling, sisi;
        sisi = 8;
        luas = sisi*sisi;
        keliling = 4*sisi;
        
        System.out.println("Luas persegi : " + luas);
        System.out.println("Keliling persegi : " + keliling);
        
    }
    
}
